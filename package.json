{
  "name": "js-router-monorepo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter play dev",
    "build": "pnpm --filter @js-router/lib build",
    "build:play": "pnpm --filter play build",
    "build:all": "pnpm run build && pnpm run build:play",
    "preview": "pnpm --filter play preview"
  },
  "dependencies": {
    "@js-router/lib": "workspace:*",
    "play": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.11.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.0"
  }
}
